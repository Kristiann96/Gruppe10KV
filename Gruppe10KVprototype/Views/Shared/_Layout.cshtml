<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title> - Gruppe10KVprototype</title>
    <!--<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />-->
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/Gruppe10KVprototype.styles.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true"/>
    @await RenderSectionAsync("Styles", false)
</head>
<body>
<header>
    <nav class="navbar">
        <div class="container-fluid">
            <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">
                <img src="@Url.Content("~/Images/LogoKartverket.png")" alt="Logo"/>
            </a>


            <div id="navbar-menu">
                <ul class="navbarlist">
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">Hjem</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="MapView">Kart</a>
                    </li>
                    
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-controller="SaksbehandlerInnmelding" asp-action="Index">Saksbehandler</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="MineSaker" asp-action="MineSaker">MineSaker</a>
                    </li>
                    
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="OppdatereInnmelding" asp-action="OppdatereInnmelding">OppdaterInnmelding</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="LoggInn" asp-action="LoggInn">LoggInn</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="LandingsSide" asp-action="LandingsSide">LandingsSide</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="RegistrerDeg" asp-action="RegistrerDeg">RegistrerDeg</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="KnyttInnmeldigTilPerson" asp-action="KnyttInnmeldigTilPerson">KnyttInnmeldingTilPerson</a>
                    </li>

                    
                </ul>
            </div>
        </div>
    </nav>
</header>

<main role="main" class="pb-3">
    @RenderBody()
</main>


<footer class="border-top footer text-muted">
    <div class="container">
        &copy; 2024 - Gruppe10KVprototype - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
    </div>
</footer>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", false)
</body>
</html>